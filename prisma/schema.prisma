generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  email     String
  company   String
  reScore   Int
  riskLevel String
  answers   Json
  status    String   @default("new")
  createdAt DateTime @default(now())

  @@index([email])
  @@index([createdAt])
}
